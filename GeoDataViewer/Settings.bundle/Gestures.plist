			<array>
				<string>1 finger</string>
				<string>2 fingers</string>
				<string>3 fingers</string>
			</array>
			<key>Values</key>
			<array>
				<integer>1</integer>
				<integer>2</integer>
				<integer>3</integer>
			</array>
		</dict>
	</array>
